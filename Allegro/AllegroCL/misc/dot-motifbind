!	osfUp		:		<Key>Up
!	osfRight	:		<Key>Right
!	osfDown		:		<Key>Down
!	osfEndLine	:		<Key>R13
!	osfBeginLine	:		<Key>F27
!	osfPageUp	:		<Key>F29
!	osfPageDown	:		<Key>F35
!	osfInsert	:		<Key>Insert
!	osfAddMode	:Shift		<Key>F8
!	osfMenu		:		<Key>F4
!	osfMenuBar	:		<Key>F10
!	osfCopy		:		<Key>F16
!	osfCut		:		<Key>F20
!	osfPaste	:		<Key>F18
