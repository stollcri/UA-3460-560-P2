(def-java-class tester ()
  :public
  ((iii :type int :initform 12345) 
   (sss :type int :initform 3241 
	:flags (:static))
   ))

(def-java-macro failure (test want got)
  `(progn (System.out.print "Failed! test ")
	  (System.out.print ,test)
	  (System.out.print " Wanted: ")
	  (System.out.print ,want)
	  (System.out.print " Got: ")
	  (System.out.println ,got)))


(def-java-method check ((testnum int) (shouldbe long) (result long)) void
    :public :static
    (if*  (= shouldbe result)
        then (System.out.print "Ok ") (System.out.println testnum)
       else (failure testnum shouldbe result)))

(def-java-method check ((testnum int) (shouldbe int) (result int)) void
    :public :static
    (if*  (= shouldbe result)
        then (System.out.print "Ok ") (System.out.println testnum)
       else (failure testnum shouldbe result)))

(def-java-method check ((testnum int) (shouldbe float) (result float)) void
    :public :static
    (if*  (= shouldbe result)
       then (System.out.print "Ok ") (System.out.println testnum)
       else (failure testnum shouldbe result)))

(def-java-method check ((testnum int) (shouldbe double) (result double)) void
    :public :static
    (if* (= shouldbe result)
       then  (System.out.print "Ok ") (System.out.println testnum)
       else (failure testnum shouldbe result)))


	    

